{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "declaration": true,
        "outDir": "./build/",
        "rootDir": "."
    },
    "include": [
        "./typechain-ethers6/**/*.ts",
        "./typechain-ethers5/**/*.ts",
        "./constants/**/*.ts",
        "./tmp/index.ts"
    ]
}